{
	"name": "@skulpture/serve",
	"version": "7.4.3",
	"type": "module",
	"description": "A lightweight starting point for API applications",
	"main": "dist/index.js",
	"bin": "./scripts/scripts.mjs",
	"keywords": [
		"h3",
		"backend",
		"framework",
		"lightweight",
		"extensible",
		"api",
		"node",
		"application",
		"http2",
		"spdy"
	],
	"scripts": {
		"build": "tsc",
		"lint": "eslint --ext .ts --ignore-path .gitignore . --fix",
		"format": "prettier .  --write",
		"test": "vitest",
		"test.watch": "vitest --watch",
		"release": "rm -rf dist && rm -rf node_modules && pnpm install && pnpm build && cp -rf src/build/docker/templates dist/build/docker/templates && git push --follow-tags && pnpm publish",
		"prepare": "husky install"
	},
	"author": "Naveen Mathew",
	"license": "MIT",
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/csurf": "^1.11.2",
		"@types/express": "^4.17.14",
		"@types/inquirer": "^9.0.2",
		"@types/node": "^18.11.9",
		"@types/tmp": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"prettier": "^2.7.1",
		"vitest": "^0.24.5"
	},
	"dependencies": {
		"@graphql-tools/delegate": "^9.0.14",
		"@graphql-tools/schema": "^9.0.8",
		"@graphql-tools/stitch": "^8.7.19",
		"@graphql-tools/wrap": "^9.2.9",
		"@rollup/plugin-alias": "^4.0.2",
		"@rollup/plugin-auto-install": "^3.0.2",
		"@rollup/plugin-commonjs": "^23.0.2",
		"@rollup/plugin-json": "^5.0.1",
		"@rollup/plugin-node-resolve": "13.3.0",
		"@rollup/plugin-replace": "^5.0.1",
		"@rollup/plugin-virtual": "^3.0.1",
		"@sentry/node": "^7.17.3",
		"@sentry/tracing": "^7.17.3",
		"@tinyhttp/app": "^2.0.24",
		"c12": "^0.2.13",
		"chokidar": "^3.5.3",
		"consola": "^2.15.3",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"esbuild": "^0.15.13",
		"graphql": "^16.6.0",
		"graphql-jit": "^0.7.4",
		"graphql-sse": "^1.3.0",
		"h3": "^0.8.6",
		"helmet": "^6.0.0",
		"hookable": "^5.4.1",
		"inquirer": "^9.1.4",
		"rollup": "^2.0.0\n",
		"rollup-plugin-esbuild": "^4.10.1",
		"sofa-api": "^0.15.0",
		"tmp": "^0.2.1",
		"typescript": "^4.8.4",
		"unplugin-auto-import": "^0.11.4"
	},
	"pnpm": {
		"patchedDependencies": {
			"@rollup/plugin-auto-install@3.0.2": "patches/@rollup__plugin-auto-install@3.0.2.patch"
		}
	}
}
